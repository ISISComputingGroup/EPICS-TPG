record(mbbi, "$(P)UNITS") 
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@devTPG300.proto getUnits $(PORT)")
    field(ZRST, "mbar")
    field(ONST, "Torr")
    field(TWST, "Pa")
}

record(mbbo, "$(P)UNITS:SP") 
{
    field(DTYP, "stream")
    field(OUT,  "@devTPG300.proto setUnits $(PORT)")
    field(ZRST, "mbar")
    field(ONST, "Torr")
    field(TWST, "Pa")
}

alias("$(P)UNITS", "$(P)UNITS:SP:RBV")