# These PVs are separated from the main DB file as these commands are not used by both models

record(stringin, "$(P)FUNCTION:STATUS:RB")
{
    field(DESC, "Reading all functions' status")
    field(DTYP, "stream")
    field(SCAN, "1 second")
    field(PINI, "YES")
    field(INP, "@devTPG300.proto getFunctionsStatus($(P)FUNCTION:STATUS) $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:FUNCTION:STATUS:RB")
    field(SDIS, "$(P)DISABLE")
}

